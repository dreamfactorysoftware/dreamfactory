{
    "name": "dreamfactory/dreamfactory",
    "description": "The DreamFactory(tm) Platform.",
    "keywords": [
        "api",
        "dreamfactory",
        "laravel",
        "rest"
    ],
    "homepage": "https://www.dreamfactory.com/",
    "license": "Apache-2.0",
    "type": "project",
    "authors": [
        {
            "name": "DreamFactory Team",
            "email": "code@dreamfactory.com"
        }
    ],
    "support": {
        "email": "dspsupport@dreamfactory.com",
        "source": "https://github.com/dreamfactorysoftware/dreamfactory",
        "issues": "https://github.com/dreamfactorysoftware/dreamfactory/issues",
        "wiki": "https://wiki.dreamfactory.com",
        "guide": "https://guide.dreamfactory.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [],
    "require": {
        "php": "^7.1.3",
        "dreamfactory/df-admin-app": "~3.0.2",
        "dreamfactory/df-api-docs-ui": "~3.0.0",
        "dreamfactory/df-apidoc": "dev-laravel-upgrade",
        "dreamfactory/df-aws": "dev-laravel-upgrade",
        "dreamfactory/df-azure": "dev-laravel-upgrade",
        "dreamfactory/df-cache": "dev-laravel-upgrade",
        "dreamfactory/df-cassandra": "dev-laravel-upgrade",
        "dreamfactory/df-core": "dev-laravel-upgrade",
        "dreamfactory/df-couchbase": "dev-laravel-upgrade",
        "dreamfactory/df-couchdb": "dev-laravel-upgrade",
        "dreamfactory/df-email": "dev-laravel-upgrade",
        "dreamfactory/df-file": "dev-laravel-upgrade",
        "dreamfactory/df-filemanager-app": "~0.3.0",
        "dreamfactory/df-firebird": "dev-laravel-upgrade",
        "dreamfactory/df-git": "dev-laravel-upgrade",
        "dreamfactory/df-graphql": "dev-laravel-upgrade",
        "dreamfactory/df-oauth": "dev-laravel-upgrade",
        "dreamfactory/df-rackspace": "dev-laravel-upgrade",
        "dreamfactory/df-rws": "dev-laravel-upgrade",
        "dreamfactory/df-sqldb": "dev-laravel-upgrade",
        "dreamfactory/df-system": "dev-laravel-upgrade",
        "dreamfactory/df-user": "dev-laravel-upgrade",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "~1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "~2.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "~1.0",
        "laracasts/testdummy": "~2.0",
        "laravel/homestead": "6.6.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "beyondcode/laravel-dump-server": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "DreamFactory\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.0.x-dev"
        },
        "installer-paths": {
            "public/{$name}/": [
                "type:dreamfactory-app"
            ]
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
